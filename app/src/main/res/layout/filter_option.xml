<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:focusable="true">

    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/checkbox"
        style="@style/Text14spRegularPrimary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/app_item_margin"
        android:button="@drawable/custom_checkbox_selector"
        android:clickable="false"
        android:focusable="false"
        android:includeFontPadding="false"
        android:minHeight="0dp"
        android:paddingVertical="@dimen/app_item_padding"
        android:paddingStart="@dimen/app_item_margin"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry"
        tools:text="KHAI-TAN" />

</androidx.constraintlayout.widget.ConstraintLayout>